{
    "env": {
        "node": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],        
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
				
    }  
}
